(ns shrine.main
  (:require ["package:flutter/material.dart" :as m]
            [shrine.app :as app]
            [shrine.home :as home]
            [shrine.login :as login]
            [cljd.flutter :as f]))

(defonce app-state (atom {}))

(defn main []
  (f/run
   (m/MaterialApp
    .title "Shrine"
    .theme app/shrine-theme
    .initialRoute "/login"
    .routes {"/login" login/login-page
             "/" home/home-page})))
