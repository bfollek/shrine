(ns shrine.main
  (:require ["package:flutter/material.dart" :as m]
            ["dart:core" :as dart]
            [shrine.login :as login]
            [cljd.flutter :as f]))

(defonce app-state (atom {}))

(defn debug-print
  [msg]
  (when (:debug @app-state) (dart/print msg)))

(defn main []
  (f/run
   (m/MaterialApp
    .title "Shriner"
    .home login/login-page)))
